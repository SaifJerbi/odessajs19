{
  "name": "odessajs19-plugin-img",
  "version": "0.1.0",
  "keywords": [
    "odessajs-pluggable-app",
    "plugin",
    "demo",
    "odessajs19"
  ],
  "title": "My First Plugin",
  "description": "A demo extension for the platform",
  "entryPointFile": "client/bundles/odessajs19-plugin-img.umd.js",
  "entryPointModule": "MyPluginModule",
  "entryPointCmp": "app-my-plugin",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "yarn build:client && yarn build:server && yarn copy-package",
    "build:client": "ts-node ng-packagr/build.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "copy-package": "cp package.json ./dist"
  },
  "main": "./server",
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@types/express": "^4.17.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/build-ng-packagr": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "src/client/app/my-plugin/public-api.ts",
      "moduleId": "my-plugin"
    },
    "whitelistedNonPeerDependencies": [
      "."
    ],
    "dest": "./dist/client"
  }
}
